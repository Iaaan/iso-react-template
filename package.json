{
  "name": "iso-react-template",
  "version": "0.0.0",
  "description": "NPM project template for creating isomorphic web apps using express, iso, and react.",
  "repository": "github:iaaan/iso-react-template",
  "scripts": {
    "compile:server": "babel server/ -d server_build/",
    "bundle:dev": "webpack --watch --config ./webpack/dev.config.js",
    "bundle:prod": "webpack --config ./webpack/prod.config.js",
    "build:prod": "npm run bundle:prod; npm run compile:server;",
    "start:dev": "concurrently --kill-others \"npm run bundle:dev\" \"nodemon --exec babel-node server/\"",
    "start:prod": "npm run build:prod; node server_build/",
    "lint": "eslint app server"
  },
  "author": "Ian Ainley <ian.ainley@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "classnames": "^2.2.3",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "history": "^2.0.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "iso": "^5.2.0",
    "lodash": "^4.9.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "postcss-nested": "^1.0.0",
    "precss": "^1.4.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.0.1",
    "react-router": "^2.0.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.10.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "^2.0.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "jsdom": "^8.3.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1"
  }
}
