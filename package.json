{
  "name": "react-iso-template",
  "version": "0.0.0",
  "description": "NPM project template for creating isomorphic web apps using express, iso, and react.",
  "main": "index.js",
  "scripts": {
    "build_server": "babel server/ -d build/",
    "bundle_dev": "webpack --config ./webpack/dev.config.js",
    "bundle_prod": "webpack --config ./webpack/prod.config.js",
    "build": "npm run bundle_prod; npm run build_server;",
    "build_dev": "npm run bundle_dev; npm run build_server;",
    "start_dev": "npm run build_dev; node build/index.js"
  },
  "author": "Ian Ainley <ian.ainley@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "express": "^4.13.4",
    "history": "^2.0.1",
    "iso": "^5.2.0",
    "lodash": "^4.9.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.0.1",
    "react-router": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jsdom": "^8.3.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.10.0"
  }
}
